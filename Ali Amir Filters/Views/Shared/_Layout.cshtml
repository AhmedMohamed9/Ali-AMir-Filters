<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Ali AMir", "Index", "Coustmers", new { area = "" }, new { @class = "navbar-brand name" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" id="myDIV">
                    <li>@Html.ActionLink("العملاء", "Index", "Coustmers")</li>
                    <li>@Html.ActionLink("مواعيد الصيانه", "Index", "syanas")</li>
                    <li>@Html.ActionLink("الحسابات", "Index", "accountances")</li>
                    <li>@Html.ActionLink("الفلاتر", "Index", "Filterssses")</li>
                    <li>@Html.ActionLink("الشمعات", "Index", "Candels")</li>




                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer class="panel-footer navbar-fixed-bottom">
            <p>&copy; @DateTime.Now.Year - AM</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)

    <script src="~/Scripts/JavaScript.js"></script>




</body>
</html>
